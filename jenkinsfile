pipeline {
	agent any
	    //def rtMaven = Artifactory.newMavenBuild()
//定义mvn环境
    def mvnHome = tool '/var/jenkins_home/apache-maven-3.6.0'
    env.PATH = "${mvnHome}/bin:${env.PATH}"
	stages {
		stage('Build'){
			steps {
				sh "mvn clean package"
				echo 'Building...1'
			}
		}
		stage('Test') {
			steps {
				echo 'Testing...2'
			}
		}
		stage('Deploy') {
			steps {
				echo 'Deploying...3'
			}
		}
	
	}
}