pipeline {
	agent any
	    def rtMaven = Artifactory.newMavenBuild()

	stages {
		stage('Build'){
			steps {
				rtMaven.run pom: 'pom.xml', goals: 'clean install', buildInfo: buildInfo
			
				echo 'Building...1'
			}
		}
		stage('Test') {
			steps {
				echo 'Testing...2'
			}
		}
		stage('Deploy') {
			steps {
				echo 'Deploying...3'
			}
		}
	
	}
}