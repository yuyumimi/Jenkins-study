pipeline {
	agent any
	    //def rtMaven = Artifactory.newMavenBuild()
//定义mvn环境
    def mvnHome = '/var/jenkins_home/apache-maven-3.6.0/bin/'

	stages {
		stage('Build'){
			steps {
				sh mvnHome+"mvn clean package"
				echo 'Building...1'
			}
		}
		stage('Test') {
			steps {
				echo 'Testing...2'
			}
		}
		stage('Deploy') {
			steps {
				echo 'Deploying...3'
			}
		}
	
	}
}